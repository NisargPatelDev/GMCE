        <script>
            $(document).on('click', '#Logout', function () {
                swal({
                    title: "Are you sure?",
                    text: "You Want To Logout",
                    icon: "warning",
                    buttons: true,
                    dangerMode: true,
                })
                    .then((willDelete) => {
                        if (willDelete) {
                            $.ajax({
                                url: "/Home/AbandonSession",
                                type: "POST",
                                dataType: "json",
                                success: function () {
                                    window.location.replace("http://gmce.edu/home/login");
                                }
                            });

                        } else {
                            swal("You Stay Logged In");
                        }
                    });

            })
